{
  "title": "Micrometer - MÃ©tricas del sistema",
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "graph",
      "title": "Pagos (OK / Error)",
      "id": 1,
      "datasource": "InfluxDB",
      "lines": false,
      "bars": true,
      "fill": 80,
      "barWidth": 1,
      "legend": {
        "show": true,
        "alignAsTable": true
      },
      "targets": [
        {
          "refId": "A",
          "measurement": "pagos",
          "groupBy": [
            { "type": "time", "params": ["$interval"] },
            { "type": "tag", "params": ["estado"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC",
          "alias": "Pago {{estado}}"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Transferencias",
