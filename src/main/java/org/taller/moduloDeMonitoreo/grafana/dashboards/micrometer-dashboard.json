{
  "title": "Micrometer - MÃ©tricas del sistema",
  "timezone": "browser",
  "schemaVersion": 30,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Pagos (OK / Error)",
      "id": 1,
      "datasource": "InfluxDB",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0.5,
            "fillOpacity": 80
          }
        }
      },
      "targets": [
        {
          "refId": "A",
          "measurement": "pagos",
          "groupBy": [
            { "type": "time", "params": ["$__interval"] },
            { "type": "tag", "params": ["estado"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC",
          "alias": "Pago {{estado}}"
        }
      ],
      "legend": {
        "show": true,
        "displayMode": "table"
      }
    },
    {
      "type": "timeseries",
      "title": "Transferencias",
      "id": 2,
      "datasource": "InfluxDB",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0.5,
            "fillOpacity": 80
          }
        }
      },
      "targets": [
        {
          "refId": "B",
          "measurement": "transferenciaCounter",
          "groupBy": [
            { "type": "time", "params": ["$__interval"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC"
        }
      ],
      "legend": {
        "show": true,
        "displayMode": "table"
      }
    },
    {
      "type": "timeseries",
      "title": "Reclamos (Totales y Negativos)",
      "id": 3,
      "datasource": "InfluxDB",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0.5,
            "fillOpacity": 80
          }
        }
      },
      "targets": [
        {
          "refId": "A",
          "measurement": "reclamoCounter",
          "tags": [
            { "key": "etiqueta", "operator": "=", "value": "TOTAL" }
          ],
          "groupBy": [
            { "type": "time", "params": ["$__interval"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC",
          "alias": "Reclamos Totales"
        },
        {
          "refId": "B",
          "measurement": "reclamoCounter",
          "tags": [
            { "key": "etiqueta", "operator": "=", "value": "NEGATIVO" }
          ],
          "groupBy": [
            { "type": "time", "params": ["$__interval"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC",
          "alias": "Reclamos Negativos"
        }
      ],
      "legend": {
        "show": true,
        "displayMode": "table"
      }
    },
    {
      "type": "timeseries",
      "title": "Reportes",
      "id": 4,
      "datasource": "InfluxDB",
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "bars",
            "barAlignment": 0.5,
            "fillOpacity": 80
          }
        }
      },
      "targets": [
        {
          "refId": "D",
          "measurement": "reporteCounter",
          "groupBy": [
            { "type": "time", "params": ["$__interval"] }
          ],
          "select": [
            [
              { "type": "field", "params": ["value"] },
              { "type": "sum", "params": [] }
            ]
          ],
          "orderByTime": "ASC"
        }
      ],
      "legend": {
        "show": true,
        "displayMode": "table"
      }
    }
  ]
}
